#include <iostream>
using namespace std;
int main(){
 
    char a[4][4];
 
    for(int i = 0; i < 4; i++){
        for(int j = 0; j < 4; j++){
            cin >> a[i][j];
        }
    }
 
    if(a[0][0] == 'B' && a[0][1] == 'B' && a[1][0] == 'B' && a[1][1] == 'B'){
        cout << "No";
    }
    else if(a[0][1] == 'B' && a[0][2] == 'B' && a[1][1] == 'B' && a[1][2] == 'B'){
        cout << "No";
    }
    else if(a[0][2] == 'B' && a[0][3] == 'B' && a[1][2] == 'B' && a[1][3] == 'B'){
        cout << "No";
    }
    else if(a[1][0] == 'B' && a[1][1] == 'B' && a[2][0] == 'B' && a[2][1] == 'B'){
        cout << "No";
    }
    else if(a[1][2] == 'B' && a[1][1] == 'B' && a[2][2] == 'B' && a[2][1] == 'B'){
        cout << "No";
    }
    else if(a[2][2] == 'B' && a[2][3] == 'B' && a[1][2] == 'B' && a[1][3] == 'B'){
        cout << "No";
    }
    else if(a[2][0] == 'B' && a[2][1] == 'B' && a[3][0] == 'B' && a[3][1] == 'B'){
        cout << "No";
    }
    else if(a[2][1] == 'B' && a[2][2] == 'B' && a[3][1] == 'B' && a[3][2] == 'B'){
        cout << "No";
    }
    else if(a[2][2] == 'B' && a[2][3] == 'B' && a[3][2] == 'B' && a[3][3] == 'B'){
        cout << "No";
    }
    else if(a[0][0]=='B' && a[0][1]=='B' && a[0][2]=='B' && a[0][3]=='B' && a[1][0]=='B' && a[1][1]=='B' && a[1][2]=='B' && a[1][3]=='B' && a[2][0]=='B' && a[2][1]=='B' && a[2][2]=='B' && a[2][3]=='B' && a[3][0]=='B' && a[3][1]=='B' && a[3][2]=='B' && a[3][3]=='B'){
        cout << "No";
    }
    else if(a[0][0] == 'W' && a[0][1] == 'W' && a[1][0] == 'W' && a[1][1] == 'W'){
        cout << "No";
    }
    else if(a[0][1] == 'W' && a[0][2] == 'W' && a[1][1] == 'W' && a[1][2] == 'W'){
        cout << "No";
    }
    else if(a[0][2] == 'W' && a[0][3] == 'W' && a[1][2] == 'W' && a[1][3] == 'W'){
        cout << "No";
    }
    else if(a[1][0] == 'W' && a[1][1] == 'W' && a[2][0] == 'W' && a[2][1] == 'W'){
        cout << "No";
    }
    else if(a[1][2] == 'W' && a[1][1] == 'W' && a[2][2] == 'W' && a[2][1] == 'W'){
        cout << "No";
    }
    else if(a[2][2] == 'W' && a[2][3] == 'W' && a[1][2] == 'W' && a[1][3] == 'W'){
        cout << "No";
    }
    else if(a[2][0] == 'W' && a[2][1] == 'W' && a[3][0] == 'W' && a[3][1] == 'W'){
        cout << "No";
    }
    else if(a[2][1] == 'W' && a[2][2] == 'W' && a[3][1] == 'W' && a[3][2] == 'W'){
        cout << "No";
    }
    else if(a[2][2] == 'W' && a[2][3] == 'W' && a[3][2] == 'W' && a[3][3] == 'W'){
        cout << "No";
    }
    else if(a[0][0]=='W' && a[0][1]=='W' && a[0][2]=='W' && a[0][3]=='W' && a[1][0]=='W' && a[1][1]=='W' && a[1][2]=='W' && a[1][3]=='W' && a[2][0]=='W' && a[2][1]=='W' && a[2][2]=='W' && a[2][3]=='W' && a[3][0]=='W' && a[3][1]=='W' && a[3][2]=='W' && a[3][3]=='W'){
        cout << "No";
    }
    else {
        cout << "Yes";
    }
 
    return 0;
}
