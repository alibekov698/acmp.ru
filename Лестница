#include <iostream>
using namespace std;
int main(){
 
    int n;
    cin >> n;
    int t = n;
    for(int i = 1; i <= n; i++){
        t = t - i;
        if(t == 0){
            cout << i;
            break;
        }
        else if(t < 0){
            cout << i - 1;
            break;
        }
    }
 
    return 0;
}
