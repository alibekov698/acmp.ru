#include <iostream>
using namespace std;
int main(){
 
    char a[3][3];
 
    for(int i = 0; i < 3; i++){
        for(int j = 0; j < 3; j++){
        cin >> a[i][j];
        }
    }
    //for X
    if(a[0][0] == 'X' && a[1][0] == 'X' && a[2][0] == 'X'){
        cout << "Win";
    }
    else if(a[0][1] == 'X' && a[1][1] == 'X' && a[2][1] == 'X'){
        cout << "Win";
    }
    else if(a[0][2] == 'X' && a[1][2] == 'X' && a[2][2] == 'X'){
        cout << "Win";
    }
    else if(a[0][0] == 'X' && a[0][1] == 'X' && a[0][2] == 'X'){
        cout << "Win";
    }
    else if(a[1][0] == 'X' && a[1][1] == 'X' && a[1][2] == 'X'){
        cout << "Win";
    }
    else if(a[2][0] == 'X' && a[2][1] == 'X' && a[2][2] == 'X'){
        cout << "Win";
    }
    else if(a[0][2] == 'X' && a[1][1] == 'X' && a[2][0] == 'X'){
        cout << "Win";
    }
    else if(a[0][0] == 'X' && a[1][1] == 'X' && a[2][2] == 'X'){
        cout << "Win";
    }
    // for O
    else if(a[0][0] == 'O' && a[1][0] == 'O' && a[2][0] == 'O'){
        cout << "Lose";
    }
    else if(a[0][1] == 'O' && a[1][1] == 'O' && a[2][1] == 'O'){
        cout << "Lose";
    }
    else if(a[0][2] == 'O' && a[1][2] == 'O' && a[2][2] == 'O'){
        cout << "Lose";
    }
    else if(a[0][0] == 'O' && a[0][1] == 'O' && a[0][2] == 'O'){
        cout << "Lose";
    }
    else if(a[1][0] == 'O' && a[1][1] == 'O' && a[1][2] == 'O'){
        cout << "Lose";
    }
    else if(a[2][0] == 'O' && a[2][1] == 'O' && a[2][2] == 'O'){
        cout << "Lose";
    }
    else if(a[0][2] == 'O' && a[1][1] == 'O' && a[2][0] == 'O'){
        cout << "Lose";
    }
    else if(a[0][0] == 'O' && a[1][1] == 'O' && a[2][2] == 'O'){
        cout << "Lose";
    } else {
        cout << "Draw";
    }
 
    return 0;
}
