#include <iostream>
using namespace std;
int main ()
{
    int hh, mm;
    char ch;
    cin >> hh >> ch >> mm;
 
    if (hh == 0 && mm <= 59 && mm >= 0)
        cout << "01:10";
    else if (hh == 1 && mm <= 9 && mm >= 0)
        cout << "01:10";
 
    else if (hh == 1 && mm <= 59 && mm >= 10)
        cout << "02:20";
    else if (hh == 2 && mm <= 19 && mm >= 0)
        cout << "02:20";
 
    else if (hh == 2 && mm <= 59 && mm >= 20)
        cout << "03:30";
    else if (hh == 3 && mm <= 29 && mm >= 0)
        cout << "03:30";
 
    else if (hh == 3 && mm <= 59 && mm >= 30)
        cout << "04:40";
    else if (hh == 4 && mm <= 39 && mm >= 0)
        cout << "04:40";
 
    else if (hh == 4 && mm <= 59 && mm >= 40)
        cout << "05:50";
    else if (hh == 5 && mm <= 49 && mm >= 0)
        cout << "05:50";
 
    else if (hh == 5 && mm <= 59 && mm >= 50)
        cout << "10:01";
    else if (hh >= 6 && hh <= 9 && mm <= 59 && mm >= 0)
        cout << "10:01";
    else if (hh == 10 && mm == 0)
        cout << "10:01";
 
    else if (hh == 10 && mm <= 59 && mm >= 01)
        cout << "11:11";
    else if (hh == 11 && mm <= 10 && mm >= 0)
        cout << "11:11";
 
    else if (hh == 11 && mm <= 59 && mm >= 11)
        cout << "12:21";
    else if (hh == 12 && mm <= 20 && mm >= 0)
        cout << "12:21";
 
    else if (hh == 12 && mm <= 59 && mm >= 21)
        cout << "13:31";
    else if (hh == 13 && mm <= 30 && mm >= 0)
        cout << "13:31";
 
    else if (hh == 13 && mm <= 59 && mm >= 31)
        cout << "14:41";
    else if (hh == 14 && mm <= 40 && mm >= 0)
        cout << "14:41";
 
    else if (hh == 14 && mm <= 59 && mm >= 41)
        cout << "15:51";
    else if (hh == 15 && mm <= 50 && mm >= 0)
        cout << "15:51";
 
    else if (hh == 15 && mm <= 59 && mm >= 51)
        cout << "20:02";
    else if (hh >= 16 && hh <= 19 && mm <= 59 && mm >= 0)
        cout << "20:02";
    else if (hh == 20 && mm <= 1 && mm >= 0)
        cout << "20:02";
 
    else if (hh <= 20 && mm <= 59 && mm >= 02)
        cout << "21:12";
    else if (hh == 21 && mm <= 11 && mm >= 0)
        cout << "21:12";
 
    else if (hh <= 21 && mm <= 59 && mm >= 12)
        cout << "22:22";
    else if (hh == 22 && mm <= 21 && mm >= 0)
        cout << "22:22";
 
    else if (hh <= 22 && mm <= 59 && mm >= 22)
        cout << "23:32";
    else if (hh == 23 && mm <= 31 && mm >= 0)
        cout << "23:32";
 
    else if (hh <= 23 && mm <= 59 && mm >= 32)
        cout << "00:00";
 
    return 0;
}
